{
  "extends": [
  	"fullstack",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "semi": [1, "always"],
    "no-console": 0,
    "react/prop-types": 0
  },
  "env": {
    "jest": true
  }
}
